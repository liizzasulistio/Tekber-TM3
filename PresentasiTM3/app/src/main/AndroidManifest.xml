<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.liizzasulistio.tekber.kelompok3.presentasitm3">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PresentasiTM3">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" /> <!-- Entry Point -->
                <category android:name="android.intent.category.LAUNCHER" /> <!-- Activity Launcher -->
            </intent-filter>
        </activity>

        <activity android:name=".ResultActivity"
            android:parentActivityName=".MainActivity"
            android:label="Move With Data"/>

        <activity android:name=".OtherActivity"
            android:parentActivityName=".MainActivity"
            android:label="Move Without Data"/>
    </application>

</manifest>